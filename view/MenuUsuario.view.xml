<mvc:View 
    xmlns="sap.m"
    xmlns:u="sap.uxap" 
    xmlns:l="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:f="sap.ui.layout.form" 
    controllerName="sap.ui.entelantenas.controller.MenuUsuario">
    <Page title="Gestión de usuarios"
        showHeader="true"
        enableScrolling="true">

        <customHeader>
            <Toolbar>
                <Button type="Back" press="onPressBack" />
                <ToolbarSpacer/>
                <Title text="Gestión de usuario" level="H2"/>
                <ToolbarSpacer/>
                <Button icon="sap-icon://log" press="OnPressLogout" ariaLabelledBy="editButtonLabel"/>
            </Toolbar>
        </customHeader>

        <GenericTile class="sapUiLargeMarginBegin sapUiLargeMarginTop tileLayout" header="Nuevo Usuario"
        subheader="" press="onPressNuevoUsuario">
            <tileContent>
                <TileContent unit="" footer="Ingresar">
                    <content>
                        <ImageContent src="sap-icon://add-employee"/>
                    </content>
                </TileContent>
            </tileContent>
        </GenericTile>
        
        <GenericTile class="sapUiLargeMarginBegin sapUiLargeMarginTop tileLayout" header="Listar Usuarios"
        subheader="" press="onPressListarUsuario">
            <tileContent>
                <TileContent unit="" footer="Ingresar">
                    <content>
                        <ImageContent src="sap-icon://account"/>
                    </content>
                </TileContent>
            </tileContent>
        </GenericTile>

    </Page>
</mvc:View>