<mvc:View 
    xmlns="sap.m"
    xmlns:u="sap.uxap" 
    xmlns:l="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:f="sap.ui.layout.form" 
    controllerName="sap.ui.entelantenas.controller.MenuTecnologia">
    <Page 
        showHeader="true"
        enableScrolling="true">

        <customHeader>
            <Toolbar>
                <Button type="Back" press="onPressBack" />
                <ToolbarSpacer/>
                <Title text="Gestión de tecnología" level="H2"/>
                <ToolbarSpacer/>
                <Button icon="sap-icon://log" press="OnPressLogout" ariaLabelledBy="editButtonLabel"/>
            </Toolbar>
        </customHeader>

        <GenericTile class="sapUiLargeMarginBegin sapUiLargeMarginTop tileLayout" header="Nueva Tecnologia"
        subheader="" press="onPressNuevoTecnologia">
            <tileContent>
                <TileContent unit="" footer="Ingresar">
                    <content>
                        <ImageContent src="sap-icon://add"/>
                    </content>
                </TileContent>
            </tileContent>
        </GenericTile>
        
        <GenericTile class="sapUiLargeMarginBegin sapUiLargeMarginTop tileLayout" header="Listar Tecnologias"
        subheader="" press="onPressListarTecnologia">
            <tileContent>
                <TileContent unit="" footer="Ingresar">
                    <content>
                        <ImageContent src="sap-icon://list"/>
                    </content>
                </TileContent>
            </tileContent>
        </GenericTile>

    </Page>
</mvc:View>